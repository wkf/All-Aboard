<!DOCTYPE html>
<html>
  <head>
    <title><%- title %></title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <link href='/styles/application.css' rel='stylesheet'/>
  </head>

  <body>
    <%- body %>

    <div id='main-region'></div>

    <!-- A few key dependencies, linked in order -->

    <!-- Fonts -->
    <script type="text/javascript" src="//use.typekit.net/big2tqi.js"></script>
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

    <!-- Bring in the socket.io client -->
    <script type="text/javascript" src="/js/socket.io.js"></script>

    <!-- then beef it up with some convenience logic for talking to Sails.js -->
    <script type="text/javascript" src="/js/sails.io.js"></script>

    <!-- listen on socket.io for incoming messages -->
    <script type="text/javascript" src="/js/app.js"></script>

    <!-- vendor -->
    <script type="text/javascript" src="/js/vendor/jquery.js"></script>
    <script type="text/javascript" src="/js/vendor/underscore.js"></script>
    <script type="text/javascript" src="/js/vendor/backbone.js"></script>
    <script type="text/javascript" src="/js/vendor/backbone.marionette.js"></script>

    <!-- App -->
      <!-- Templates -->
      <script type="text/javascript" src="/jst.js"></script>

      <!-- Ext -->
      <script type="text/javascript" src="/js/ext/backbone.marionette.js"></script>

      <!-- Core -->
      <script type="text/javascript" src="/js/aa.js"></script>

      <!-- Models -->
      <script type="text/javascript" src="/js/models/card.js"></script>
      <script type="text/javascript" src="/js/models/card-group.js"></script>
      <script type="text/javascript" src="/js/models/clue.js"></script>
      <script type="text/javascript" src="/js/models/clue-group.js"></script>

      <!-- Collections -->
      <script type="text/javascript" src="/js/collections/cards.js"></script>
      <script type="text/javascript" src="/js/collections/card-groups.js"></script>
      <script type="text/javascript" src="/js/collections/clues.js"></script>
      <script type="text/javascript" src="/js/collections/clue-groups.js"></script>

      <!-- Views -->
      <script type="text/javascript" src="/js/views/card.js"></script>

      <!-- Player -->
      <script type="text/javascript" src="/js/apps/player/player-app.js"></script>
      <script type="text/javascript" src="/js/apps/player/show/show-controller.js"></script>
      <script type="text/javascript" src="/js/apps/player/show/views/player-layout.js"></script>

      <!-- Cards -->
      <script type="text/javascript" src="/js/apps/player/cards/cards-app.js"></script>
      <script type="text/javascript" src="/js/apps/player/cards/list/list-controller.js"></script>
      <script type="text/javascript" src="/js/apps/player/cards/list/views/card-groups.js"></script>

      <!-- Clues -->
      <script type="text/javascript" src="/js/apps/player/clues/clues-app.js"></script>
      <script type="text/javascript" src="/js/apps/player/clues/list/list-controller.js"></script>
      <script type="text/javascript" src="/js/apps/player/clues/list/views/clue-groups.js"></script>

      <!-- Board -->
      <script type="text/javascript" src="/js/apps/board/board-app.js"></script>
      <script type="text/javascript" src="/js/apps/board/show/show-controller.js"></script>
      <script type="text/javascript" src="/js/apps/board/show/views/board-layout.js"></script>

      <script type="text/javascript" src="/js/main.js"></script>
    </body>
</html>
